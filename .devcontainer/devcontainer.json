{
	// "dockerComposeFile": "redis/docker-compose.yml",
	// "dockerComposeFile": "mongo/docker-compose.yml",
	"dockerComposeFile": "jdbc/docker-compose.yml",
	"service": "devcontainer",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}/teste-jdbc", // Change workspace subfolder according to desired project
	// "features": {
	// 	"ghcr.io/devcontainers-contrib/features/mongosh-homebrew:1": {} // Mongosh (Shell) Feature
	// },
	"customizations": {
		"vscode": {
			"extensions": [
				"cweijan.vscode-mysql-client2" // MySQL Explorer for VS Code
				// "mongodb.mongodb-vscode" // MongoDB Explorer for VS Code
			],
			"settings": {
				"mysql.connections": [
					{
						"name": "mysql-container",
						"host": "db",
						"port": 3306,
						"user": "root",
						"password": "password",
						"database": "",
						"ssl": false
					}
				]
			}
		}
	}
}